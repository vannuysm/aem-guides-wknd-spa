<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="nt:unstructured" jcr:title="Cell Component" sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/container">
        <layout jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns" margin="{Boolean}false"/>
        <items jcr:primaryType="nt:unstructured">
            <columnProperties jcr:primaryType="nt:unstructured" jcr:title="Content" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <tabs jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/tabs" maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <content jcr:primaryType="nt:unstructured" jcr:title="Content" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <text jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Text" name="./text"/>
                                                    <subText jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Sub Text" name="./subText"/>
                                                    <imageReference jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathfield" fieldLabel="Image Reference" name="./imageReference" rootPath="/content/dam"/>
                                                    <contentReference jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathfield" fieldLabel="Content Reference" name="./contentReference" rootPath="/content/dam"/>
                                                    <openIn jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" fieldLabel="Open In" name="./openIn">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <modal jcr:primaryType="nt:unstructured" text="Modal" value="modal"/>
                                                            <newTab jcr:primaryType="nt:unstructured" text="New Tab" value="newTab"/>
                                                            <sameTab jcr:primaryType="nt:unstructured" text="Same Tab" value="sameTab"/>
                                                        </items>
                                                    </openIn>
                                                </items>
                                            </column>
                                        </items>
                                    </content>
                                    <design jcr:primaryType="nt:unstructured" jcr:title="Design" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <cellBorderColor jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Cell Border Color" name="./cellBorderColor"/>
                                                    <cellBorderWidth jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Cell Border Width" name="./cellBorderWidth"/>
                                                    <cellBorderStyleLeft jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/checkbox" name="./borderLeft" text="Border Left" value="true"/>
                                                    <cellBorderStyleTop jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/checkbox" name="./borderTop" text="Border Top" value="true"/>
                                                    <cellBorderStyleRight jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/checkbox" name="./borderRight" text="Border Right" value="true"/>
                                                    <cellBorderStyleBottom jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/checkbox" name="./borderBottom" text="Border Bottom" value="true"/>
                                                    <contentAlignment jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" fieldLabel="Content Alignment" name="./contentAlignment">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <left jcr:primaryType="nt:unstructured" text="Left" value=""/>
                                                            <center jcr:primaryType="nt:unstructured" text="Center" value="align-center-middle"/>
                                                            <right jcr:primaryType="nt:unstructured" text="Right" value="flex-dir-row-reverse"/>
                                                        </items>
                                                    </contentAlignment>
                                                    <backgroundColor granite:class="cq-dialog-dropdown-showhide" jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" fieldLabel="Background Color" name="./columnHoverColor">
                                                        <granite:data jcr:primaryType="nt:unstructured" cq-dialog-dropdown-showhide-target=".custColorTarget"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <Default jcr:primaryType="nt:unstructured" text="Default Color" value=""/>
                                                            <primary jcr:primaryType="nt:unstructured" text="Primary" value="primary"/>
                                                            <secondary jcr:primaryType="nt:unstructured" text="Secondary" value="secondary"/>
                                                            <success jcr:primaryType="nt:unstructured" text="Success" value="success"/>
                                                            <alert jcr:primaryType="nt:unstructured" text="Alert" value="alert"/>
                                                            <warning jcr:primaryType="nt:unstructured" text="Warning" value="warning"/>
                                                            <color1 jcr:primaryType="nt:unstructured" text="color1" value="color1"/>
                                                            <color2 jcr:primaryType="nt:unstructured" text="color2" value="color2"/>
                                                            <color3 jcr:primaryType="nt:unstructured" text="color3" value="color3"/>
                                                            <color4 jcr:primaryType="nt:unstructured" text="color4" value="color4"/>
                                                            <color5 jcr:primaryType="nt:unstructured" text="color5" value="color5"/>
                                                            <color6 jcr:primaryType="nt:unstructured" text="color6" value="color6"/>
                                                            <color7 jcr:primaryType="nt:unstructured" text="color7" value="color7"/>
                                                            <color8 jcr:primaryType="nt:unstructured" text="color8" value="color8"/>
                                                            <color9 jcr:primaryType="nt:unstructured" text="color9" value="color9"/>
                                                            <color10 jcr:primaryType="nt:unstructured" text="color10" value="color10"/>
                                                            <color11 jcr:primaryType="nt:unstructured" text="color11" value="color11"/>
                                                            <color12 jcr:primaryType="nt:unstructured" text="color12" value="color12"/>
                                                            <color13 jcr:primaryType="nt:unstructured" text="color13" value="color13"/>
                                                            <color14 jcr:primaryType="nt:unstructured" text="color14" value="color14"/>
                                                            <color15 jcr:primaryType="nt:unstructured" text="color15" value="color15"/>
                                                            <color16 jcr:primaryType="nt:unstructured" text="color16" value="color16"/>
                                                            <custom jcr:primaryType="nt:unstructured" text="Custom" value="custom"/>
                                                        </items>
                                                    </backgroundColor>
                                                    <customBackgroundColor granite:class="custColorTarget " jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data jcr:primaryType="nt:unstructured" showhidetargetvalue="custom"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <customColor jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Custom Background Color" name="./customBackgroundColor"/>
                                                        </items>
                                                    </customBackgroundColor>
                                                    <fontColor jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Font Color" name="./fontColor"/>
                                                </items>
                                            </column>
                                        </items>
                                    </design>
                                    <configuration jcr:primaryType="nt:unstructured" jcr:title="Configuration" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <type jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" fieldLabel="Type" name="./type">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <content jcr:primaryType="nt:unstructured" text="Content" value="content"/>
                                                            <link jcr:primaryType="nt:unstructured" text="Link" value="linkCell"/>
                                                            <formTitle jcr:primaryType="nt:unstructured" text="Form Title" value="formTitle"/>
                                                            <columnHeader jcr:primaryType="nt:unstructured" text="Column Header" value="columnHeader"/>
                                                            <sectionHeader jcr:primaryType="nt:unstructured" text="Section Header" value="sectionHeader"/>
                                                        </items>
                                                    </type>
                                                    <ID jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="ID" name="./id"/>
                                                    <vertheading jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/heading" level="3" text="Vertical Spacing"/>
                                                    <marginBottomDesktop jcr:primaryType="nt:unstructured" jcr:title="Desktop Margin Bottom" sling:resourceType="granite/ui/components/foundation/include" name="./marginBottomDesk" path="base-app/components/content/design/cq:dialog/content/items/marginBottomDesktop"/>
                                                    <marginTopDesktop jcr:primaryType="nt:unstructured" jcr:title="Desktop Margin Top" sling:resourceType="granite/ui/components/foundation/include" name="./marginTopDesk" path="base-app/components/content/design/cq:dialog/content/items/marginTopDesktop"/>
                                                    <paddingBottomDesktop jcr:primaryType="nt:unstructured" jcr:title="Desktop padding Bottom" sling:resourceType="granite/ui/components/foundation/include" name="./paddingBottomDesk" path="base-app/components/content/design/cq:dialog/content/items/paddingBottomDesktop"/>
                                                    <paddingTopDesktop jcr:primaryType="nt:unstructured" jcr:title="Desktop padding Top" sling:resourceType="granite/ui/components/foundation/include" name="./paddingTopDesk" path="base-app/components/content/design/cq:dialog/content/items/paddingTopDesktop"/>
                                                    <marginBottomtablet jcr:primaryType="nt:unstructured" jcr:title="tablet Margin Bottom" sling:resourceType="granite/ui/components/foundation/include" name="./marginBottomtablet" path="base-app/components/content/design/cq:dialog/content/items/marginBottomtablet"/>
                                                    <marginToptablet jcr:primaryType="nt:unstructured" jcr:title="tablet Margin Top" sling:resourceType="granite/ui/components/foundation/include" name="./marginToptablet" path="base-app/components/content/design/cq:dialog/content/items/marginToptablet"/>
                                                    <paddingBottomtablet jcr:primaryType="nt:unstructured" jcr:title="tablet padding Bottom" sling:resourceType="granite/ui/components/foundation/include" name="./paddingBottomtablet" path="base-app/components/content/design/cq:dialog/content/items/paddingBottomtablet"/>
                                                    <paddingToptablet jcr:primaryType="nt:unstructured" jcr:title="tablet padding Top" sling:resourceType="granite/ui/components/foundation/include" name="./paddingToptablet" path="base-app/components/content/design/cq:dialog/content/items/paddingToptablet"/>
                                                    <marginBottommobile jcr:primaryType="nt:unstructured" jcr:title="mobile Margin Bottom" sling:resourceType="granite/ui/components/foundation/include" name="./marginBottommobile" path="base-app/components/content/design/cq:dialog/content/items/marginBottommobile"/>
                                                    <marginTopmobile jcr:primaryType="nt:unstructured" jcr:title="mobile Margin Top" sling:resourceType="granite/ui/components/foundation/include" name="./marginTopmobile" path="base-app/components/content/design/cq:dialog/content/items/marginTopmobile"/>
                                                    <paddingBottommobile jcr:primaryType="nt:unstructured" jcr:title="mobile padding Bottom" sling:resourceType="granite/ui/components/foundation/include" name="./paddingBottommobile" path="base-app/components/content/design/cq:dialog/content/items/paddingBottommobile"/>
                                                    <paddingTopmobile jcr:primaryType="nt:unstructured" jcr:title="mobile padding Top" sling:resourceType="granite/ui/components/foundation/include" name="./paddingTopmobile" path="base-app/components/content/design/cq:dialog/content/items/paddingTopmobile"/>
                                                    <hideheading jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/heading" level="3" text="Hide Options"/>
                                                    <hideLarge jcr:primaryType="nt:unstructured" jcr:title="Hide on large screens" sling:resourceType="granite/ui/components/foundation/include" name="./hideLarge" path="base-app/components/content/design/cq:dialog/content/items/hideLarge"/>
                                                    <hideMedium jcr:primaryType="nt:unstructured" jcr:title="Hide on medium screens" sling:resourceType="granite/ui/components/foundation/include" name="./hideMedium" path="base-app/components/content/design/cq:dialog/content/items/hideMedium"/>
                                                    <hideSmall jcr:primaryType="nt:unstructured" jcr:title="Hide on small screens" sling:resourceType="granite/ui/components/foundation/include" name="./hideSmall" path="base-app/components/content/design/cq:dialog/content/items/hideSmall"/>
                                                    <hideLogin jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/checkbox" name="./LoggedIn" text="Hide When Loggedin" value="hideLoggedIn"/>
                                                    <hideLogOut jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/checkbox" name="./LoggedOut" text="Hide When Loggedout" value="hideLoggedOut"/>
                                                </items>
                                            </column>
                                        </items>
                                    </configuration>
                                    <Analytics jcr:primaryType="nt:unstructured" jcr:title="Analytics" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <EventName jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Event Name" name="./eventName"/>
                                                    <EventCategory jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Event Category" name="./eventCategory"/>
                                                    <EventAction jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Event Action" name="./eventAction"/>
                                                    <EventLabel jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Event Label" name="./eventLabel"/>
                                                </items>
                                            </column>
                                        </items>
                                    </Analytics>
                                </items>
                            </tabs>
                        </items>
                    </column>
                </items>
            </columnProperties>
        </items>
    </content>
</jcr:root>
