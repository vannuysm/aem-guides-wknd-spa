<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="nt:unstructured" jcr:title="stockTicker" sling:resourceType="cq/gui/components/authoring/dialog">
	<content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/container">
		<layout jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns" margin="{Boolean}false" />
		<items jcr:primaryType="nt:unstructured">
			<columnProperties jcr:primaryType="nt:unstructured" jcr:title="Content" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
				<items jcr:primaryType="nt:unstructured">
					<column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
						<items jcr:primaryType="nt:unstructured">
							<tabs jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/tabs" maximized="{Boolean}true">
								<items jcr:primaryType="nt:unstructured">
									<content jcr:primaryType="nt:unstructured" jcr:title="Content" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
										<items jcr:primaryType="nt:unstructured">
											<column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
												<items jcr:primaryType="nt:unstructured">
												<contentSource granite:class="hide " jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" name="./contentSource" defaultValue="inline">
                                                        <granite:data jcr:primaryType="nt:unstructured" cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target" />
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <inline granite:hide="${cqDesign.disableInline}" jcr:primaryType="nt:unstructured" text="Inline" value="inline" />
                                                        </items>
                                                    </contentSource>
													<ticker jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Stock Ticker" fieldDescription="This is the ticker used for the API." name="./ticker" />
													<tickerText jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Symbol Text" fieldDescription="This is the Symbol on the Font end ex: NYSE" name="./tickerText" />
													<linkText jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Link Label" name="./linkText" />
													<linkUrl jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathfield" fieldLabel="Link To" name="./linkUrl" rootPath="/content" />
													<openInNewWindow jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/checkbox" fieldDescription="Check to open link in new tab" name="./linkOpenInNewWindow" text="Open in New Tab?" value="true" />
												</items>
											</column>
										</items>
									</content>
									<config jcr:primaryType="nt:unstructured" jcr:title="Configuration" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
										<items jcr:primaryType="nt:unstructured">
											<column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
												<items jcr:primaryType="nt:unstructured">
												<ID jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="ID" name="./id" />
													<time jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Refresh time in seconds" name="./time" />
													<vertheading jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/heading" level="3" text="Vertical Spacing" />
													<marginBottomDesktop jcr:primaryType="nt:unstructured" jcr:title="Desktop Margin Bottom" sling:resourceType="granite/ui/components/foundation/include" name="./marginBottomDesk" path="base-app/components/content/design/cq:dialog/content/items/marginBottomDesktop" />
													<marginTopDesktop jcr:primaryType="nt:unstructured" jcr:title="Desktop Margin Top" sling:resourceType="granite/ui/components/foundation/include" name="./marginTopDesk" path="base-app/components/content/design/cq:dialog/content/items/marginTopDesktop" />
													<paddingBottomDesktop jcr:primaryType="nt:unstructured" jcr:title="Desktop padding Bottom" sling:resourceType="granite/ui/components/foundation/include" name="./paddingBottomDesk" path="base-app/components/content/design/cq:dialog/content/items/paddingBottomDesktop" />
													<paddingTopDesktop jcr:primaryType="nt:unstructured" jcr:title="Desktop padding Top" sling:resourceType="granite/ui/components/foundation/include" name="./paddingTopDesk" path="base-app/components/content/design/cq:dialog/content/items/paddingTopDesktop" />
													<marginBottomtablet jcr:primaryType="nt:unstructured" jcr:title="tablet Margin Bottom" sling:resourceType="granite/ui/components/foundation/include" name="./marginBottomtablet" path="base-app/components/content/design/cq:dialog/content/items/marginBottomtablet" />
													<marginToptablet jcr:primaryType="nt:unstructured" jcr:title="tablet Margin Top" sling:resourceType="granite/ui/components/foundation/include" name="./marginToptablet" path="base-app/components/content/design/cq:dialog/content/items/marginToptablet" />
													<paddingBottomtablet jcr:primaryType="nt:unstructured" jcr:title="tablet padding Bottom" sling:resourceType="granite/ui/components/foundation/include" name="./paddingBottomtablet" path="base-app/components/content/design/cq:dialog/content/items/paddingBottomtablet" />
													<paddingToptablet jcr:primaryType="nt:unstructured" jcr:title="tablet padding Top" sling:resourceType="granite/ui/components/foundation/include" name="./paddingToptablet" path="base-app/components/content/design/cq:dialog/content/items/paddingToptablet" />
													<marginBottommobile jcr:primaryType="nt:unstructured" jcr:title="mobile Margin Bottom" sling:resourceType="granite/ui/components/foundation/include" name="./marginBottommobile" path="base-app/components/content/design/cq:dialog/content/items/marginBottommobile" />
													<marginTopmobile jcr:primaryType="nt:unstructured" jcr:title="mobile Margin Top" sling:resourceType="granite/ui/components/foundation/include" name="./marginTopmobile" path="base-app/components/content/design/cq:dialog/content/items/marginTopmobile" />
													<paddingBottommobile jcr:primaryType="nt:unstructured" jcr:title="mobile padding Bottom" sling:resourceType="granite/ui/components/foundation/include" name="./paddingBottommobile" path="base-app/components/content/design/cq:dialog/content/items/paddingBottommobile" />
													<paddingTopmobile jcr:primaryType="nt:unstructured" jcr:title="mobile padding Top" sling:resourceType="granite/ui/components/foundation/include" name="./paddingTopmobile" path="base-app/components/content/design/cq:dialog/content/items/paddingTopmobile" />
													<hideheading jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/heading" level="3" text="Hide Options" />
													<hideLarge jcr:primaryType="nt:unstructured" jcr:title="Hide on large screens" sling:resourceType="granite/ui/components/foundation/include" name="./hideLarge" path="base-app/components/content/design/cq:dialog/content/items/hideLarge" />
													<hideMedium jcr:primaryType="nt:unstructured" jcr:title="Hide on medium screens" sling:resourceType="granite/ui/components/foundation/include" name="./hideMedium" path="base-app/components/content/design/cq:dialog/content/items/hideMedium" />
													<hideSmall jcr:primaryType="nt:unstructured" jcr:title="Hide on small screens" sling:resourceType="granite/ui/components/foundation/include" name="./hideSmall" path="base-app/components/content/design/cq:dialog/content/items/hideSmall" />
													<hideLogin jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/checkbox" name="./LoggedIn" text="Hide When Loggedin" value="hideLoggedIn" />
													<hideLogOut jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/checkbox" name="./LoggedOut" text="Hide When Loggedout" value="hideLoggedOut" />
												</items>
											</column>
										</items>
									</config>
									<Design jcr:primaryType="nt:unstructured" jcr:title="Design" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
										<items jcr:primaryType="nt:unstructured">
											<column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
												<items jcr:primaryType="nt:unstructured">
													<symbolTextColor 
														granite:class="cq-dialog-dropdown-showhide" 
														jcr:primaryType="nt:unstructured" 
														sling:resourceType="granite/ui/components/coral/foundation/form/select" 
														fieldLabel="Symbol Text Color" 
														name="./symbol">
														<granite:data 
															jcr:primaryType="nt:unstructured" 
															cq-dialog-dropdown-showhide-target=".custSymbolTarget" />
														<datasource
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="acs-commons/components/utilities/genericlist/datasource"
                                                            path="/etc/acs-commons/lists/dynamic-color-list"/>
													</symbolTextColor>
													<custSymbolTarget
                                                        granite:class="hide custSymbolTarget"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            showhidetargetvalue="custom"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <symColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Symbol Custom Color"
                                                                name="./symColor"/>
                                                            <!-- <ctaBGColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="CTA Background Color"
                                                                name="./ctaBG"/> -->
                                                        </items>
                                                    </custSymbolTarget>

													<priceTextColor 
														granite:class="cq-dialog-dropdown-showhide" 
														jcr:primaryType="nt:unstructured" 
														sling:resourceType="granite/ui/components/coral/foundation/form/select" 
														fieldLabel="Price Text Color" 
														name="./price">
														<granite:data 
															jcr:primaryType="nt:unstructured" 
															cq-dialog-dropdown-showhide-target=".custPriceTarget" />
														<datasource
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="acs-commons/components/utilities/genericlist/datasource"
                                                            path="/etc/acs-commons/lists/dynamic-color-list"/>
													</priceTextColor>
													<custPriceTarget
                                                        granite:class="hide custPriceTarget"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            showhidetargetvalue="custom"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <symColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Price Custom Color"
                                                                name="./priceColor"/>
                                                            <!-- <ctaBGColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="CTA Background Color"
                                                                name="./ctaBG"/> -->
                                                        </items>
                                                    </custPriceTarget>

													<gainTextColor 
														granite:class="cq-dialog-dropdown-showhide" 
														jcr:primaryType="nt:unstructured" 
														sling:resourceType="granite/ui/components/coral/foundation/form/select" 
														fieldLabel="Price Gain Text Color" 
														name="./gain">
														<granite:data 
															jcr:primaryType="nt:unstructured" 
															cq-dialog-dropdown-showhide-target=".custGainTarget" />
														<datasource
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="acs-commons/components/utilities/genericlist/datasource"
                                                            path="/etc/acs-commons/lists/dynamic-color-list"/>
													</gainTextColor>
													<custGainTarget
                                                        granite:class="hide custGainTarget"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            showhidetargetvalue="custom"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <symColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Price Gain Custom Color"
                                                                name="./gainColor"/>
                                                            <!-- <ctaBGColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="CTA Background Color"
                                                                name="./ctaBG"/> -->
                                                        </items>
                                                    </custGainTarget>

													<CLR 
														granite:class="cq-dialog-dropdown-showhide" 
														jcr:primaryType="nt:unstructured" 
														sling:resourceType="granite/ui/components/coral/foundation/form/select" 
														fieldLabel="Move the symbol to the center left or right" 
														name="./clr">
														<items jcr:primaryType="nt:unstructured">
															<center jcr:primaryType="nt:unstructured" text="Center" value="align-center-middle" />
															<left jcr:primaryType="nt:unstructured" text="Left" value="" />
															<right jcr:primaryType="nt:unstructured" text="right" value="align-right" />
														</items>
													</CLR>
												</items>
											</column>
										</items>
									</Design>
									<cta jcr:primaryType="nt:unstructured" jcr:title="CTA" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" margin="{Boolean}true">
										<items jcr:primaryType="nt:unstructured">
											<column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
												<items jcr:primaryType="nt:unstructured">
												<ctaType granite:class="cq-dialog-dropdown-showhide" jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" fieldLabel="CTA Size" fieldDescription="Describes the basic format of the link. Use of Augmented will replace ownerId and membershipId with owner-specific values in the link." name="./ctaType">
														<items jcr:primaryType="nt:unstructured">
															<tiny jcr:primaryType="nt:unstructured" text="Tiny" value="tiny" />
															<small jcr:primaryType="nt:unstructured" text="Small" value="small" />
															<basic jcr:primaryType="nt:unstructured" text="Basic" value="" />
															<larg jcr:primaryType="nt:unstructured" text="Large" value="large" />
															<clear jcr:primaryType="nt:unstructured" text="Clear" value="clear" />
															<text jcr:primaryType="nt:unstructured" text="Text" value="text" />
															<expanded jcr:primaryType="nt:unstructured" text="Expanded" value="expanded" />
															<!-- <augmented jcr:primaryType="nt:unstructured" text="Augmented" value="augmented" /> -->
														</items>
													</ctaType>
													<ctafont 
														granite:class="cq-dialog-dropdown-showhide" 
														jcr:primaryType="nt:unstructured" 
														sling:resourceType="granite/ui/components/coral/foundation/form/select" 
														fieldLabel="CTA Font Color" 
														name="./ctaFont">
														<granite:data 
															jcr:primaryType="nt:unstructured" 
															cq-dialog-dropdown-showhide-target=".custColorTarget" />
														<datasource
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="acs-commons/components/utilities/genericlist/datasource"
                                                            path="/etc/acs-commons/lists/dynamic-color-list"/>
													</ctafont>
													<custColorTarget
                                                        granite:class="hide custColorTarget"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            showhidetargetvalue="custom"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <fontColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Button Font Color"
                                                                name="./fontColor"/>
                                                            <!-- <ctaBGColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="CTA Background Color"
                                                                name="./ctaBG"/> -->
                                                        </items>
                                                    </custColorTarget>
													<ctaColor granite:class="cq-dialog-dropdown-showhide" jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/select" fieldLabel="CTA Button Color" name="./ctaColor">
														<granite:data jcr:primaryType="nt:unstructured" cq-dialog-dropdown-showhide-target=".buttonCustColorTarget" />
														<datasource
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="acs-commons/components/utilities/genericlist/datasource"
                                                            path="/etc/acs-commons/lists/dynamic-color-list"/>
													</ctaColor>
													<custColorTargetButton
                                                        granite:class="hide buttonCustColorTarget"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            showhidetargetvalue="custom"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <!-- <fontColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Button Font Color"
                                                                name="./fontColor"/> -->
                                                            <ctaBGColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="CTA Background Color"
                                                                name="./ctaBG"/>
                                                        </items>
                                                    </custColorTargetButton>
													<hollow jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/checkbox" fieldDescription="Hollow CTA" name="./hollow" text="Make the CTA hollow" value="hollow " />
												</items>
											</column>
										</items>
									</cta>
									<Analytics
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Analytics"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <EventName
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Event Name"
                                                        name="./eventName"/>
                                                    <EventCategory
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Event Category"
                                                        name="./eventCategory"/>
                                                    <EventAction
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Event Action"
                                                        name="./eventAction"/>
                                                    <EventLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Event Label"
                                                        name="./eventLabel"/>
                                                </items>
                                            </column>
                                        </items>
                                    </Analytics>
								</items>
							</tabs>
						</items>
					</column>
				</items>
			</columnProperties>
		</items>
	</content>
</jcr:root>
