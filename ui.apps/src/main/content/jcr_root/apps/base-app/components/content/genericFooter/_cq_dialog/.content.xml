<?xml version="1.0" encoding="UTF-8" ?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" 
    xmlns:granite="http://www.adobe.com/jcr/granite/1.0" 
    xmlns:cq="http://www.day.com/jcr/cq/1.0" 
    xmlns:jcr="http://www.jcp.org/jcr/1.0" 
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="nt:unstructured" jcr:title="Generic Footer" sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/container">     
        <layout jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns" margin="{Boolean}false" />
        <items jcr:primaryType="nt:unstructured">
            <columnProperties jcr:primaryType="nt:unstructured" jcr:title="Content" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <tabs jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/tabs" maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <content jcr:primaryType="nt:unstructured" jcr:title="Content" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <disclaimerText jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Disclaimer Text" name="./disclaimerText"/>    
                                                    <primary-links jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/multifield" composite="{Boolean}true" fieldLabel="Primary Links">
                                                        <field jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container" name="./primaryLinks">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <link-label jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Primary Link Label" name="./linkLabel"/>
                                                                        <link-path jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathfield" fieldLabel="Primary Link Path" name="./linkPath"/>
                                                                        <link-target jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/select" fieldLabel="Link Target" name="./linkTarget">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <option1 jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/option" selected="{Boolean}true" text="Same Tab" value="_self"/>
                                                                                <option2 jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/option" text="New Tab" value="_blank"/>
                                                                            </items>
                                                                        </link-target>
                                                                        <secondary-links jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/multifield" composite="{Boolean}true" fieldLabel="Secondary Links">
                                                                            <field jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container" name="./primaryLinks">
                                                                                <items jcr:primaryType="nt:unstructured">
                                                                                    <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                                        <items jcr:primaryType="nt:unstructured">
                                                                                            <link-label jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Secondary Link Label" name="./linkLabel"/>
                                                                                            <link-path jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathfield" fieldLabel="Secondary Link Path" name="./linkPath"/>
                                                                                            <link-target jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/select" fieldLabel="Link Target" name="./linkTarget">
                                                                                                <items jcr:primaryType="nt:unstructured">
                                                                                                    <option1 jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/option" selected="{Boolean}true" text="Same Tab" value="_self"/>
                                                                                                    <option2 jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/option" text="New Tab" value="_blank"/>
                                                                                                </items>
                                                                                            </link-target>
                                                                                        </items>
                                                                                    </column>
                                                                                </items>
                                                                            </field>
                                                                        </secondary-links>
                                                                    </items>
                                                                </column>
                                                            </items>
                                                        </field>
                                                    </primary-links>
                                                    <right-top-label jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Top Right Primary Label" name="./rightTopPrimaryLabel"/>
                                                    <right-top-path jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathfield" fieldLabel="Top Right Primary Path" name="./rightTopPrimaryPath"/>
                                                    <link-target jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/select" fieldLabel="Top Right Primary Target" name="./rightTopPrimaryTarget">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <option1 jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/option" selected="{Boolean}true" text="Same Tab" value="_self"/>
                                                            <option2 jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/option" text="New Tab" value="_blank"/>
                                                        </items>
                                                    </link-target>
                                                    <right-top-secondary-links jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/multifield" composite="{Boolean}true" fieldLabel="Top Right Secondary Links">
                                                        <field jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container" name="./rightTopSecondaryLinks">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <link-label jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Secondary Label" name="./linkLabel"/>
                                                                        <link-path jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathfield" fieldLabel="Secondary Path" name="./linkPath"/>
                                                                        <link-target jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/select" fieldLabel="Link Target" name="./linkTarget">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <option1 jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/option" selected="{Boolean}true" text="Same Tab" value="_self"/>
                                                                                <option2 jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/option" text="New Tab" value="_blank"/>
                                                                            </items>
                                                                        </link-target>
                                                                    </items>
                                                                </column>
                                                            </items>
                                                        </field>
                                                    </right-top-secondary-links>
                                                    <right-bottom-secondary-links jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/multifield" composite="{Boolean}true" fieldLabel="Bottom Right Secondary Links">
                                                        <field jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container" name="./rightBottomSecondaryLinks">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <link-label jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Secondary Label" name="./linkLabel"/>
                                                                        <link-path jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathfield" fieldLabel="Secondary Path" name="./linkPath"/>
                                                                        <link-target jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/select" fieldLabel="Link Target" name="./linkTarget">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <option1 jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/option" selected="{Boolean}true" text="Same Tab" value="_self"/>
                                                                                <option2 jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/option" text="New Tab" value="_blank"/>
                                                                            </items>
                                                                        </link-target>
                                                                    </items>
                                                                </column>
                                                            </items>
                                                        </field>
                                                    </right-bottom-secondary-links>
                                                </items>
                                            </column>
                                        </items>
                                    </content>
                                    <config jcr:primaryType="nt:unstructured" jcr:title="Configuration" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" margin="{Boolean}true">

                                        <items jcr:primaryType="nt:unstructured">
                                            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <!-- Configuration component changes here. -->
                                                    <vertheading jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/heading" level="3" text="Vertical Spacing" />
                                                    <marginBottomDesktop jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/marginBottomDesktop" jcr:title="Desktop Margin Bottom" name="./marginBottomDesk" sling:resourceType="granite/ui/components/foundation/include" />
                                                    <marginTopDesktop jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/marginTopDesktop" jcr:title="Desktop Margin Top" name="./marginTopDesk" sling:resourceType="granite/ui/components/foundation/include" />
                                                    <paddingBottomDesktop jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/paddingBottomDesktop" jcr:title="Desktop padding Bottom" name="./paddingBottomDesk" sling:resourceType="granite/ui/components/foundation/include" />
                                                    <paddingTopDesktop jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/paddingTopDesktop" jcr:title="Desktop padding Top" name="./paddingTopDesk" sling:resourceType="granite/ui/components/foundation/include" />

                                                    <marginBottomtablet jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/marginBottomtablet" jcr:title="tablet Margin Bottom" name="./marginBottomtablet" sling:resourceType="granite/ui/components/foundation/include" />
                                                    <marginToptablet jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/marginToptablet" jcr:title="tablet Margin Top" name="./marginToptablet" sling:resourceType="granite/ui/components/foundation/include" />
                                                    <paddingBottomtablet jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/paddingBottomtablet" jcr:title="tablet padding Bottom" name="./paddingBottomtablet" sling:resourceType="granite/ui/components/foundation/include" />
                                                    <paddingToptablet jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/paddingToptablet" jcr:title="tablet padding Top" name="./paddingToptablet" sling:resourceType="granite/ui/components/foundation/include" />

                                                    <marginBottommobile jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/marginBottommobile" jcr:title="mobile Margin Bottom" name="./marginBottommobile" sling:resourceType="granite/ui/components/foundation/include" />
                                                    <marginTopmobile jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/marginTopmobile" jcr:title="mobile Margin Top" name="./marginTopmobile" sling:resourceType="granite/ui/components/foundation/include" />
                                                    <paddingBottommobile jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/paddingBottommobile" jcr:title="mobile padding Bottom" name="./paddingBottommobile" sling:resourceType="granite/ui/components/foundation/include" />
                                                    <paddingTopmobile jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/paddingTopmobile" jcr:title="mobile padding Top" name="./paddingTopmobile" sling:resourceType="granite/ui/components/foundation/include" />

                                                    <hideheading jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/heading" level="3" text="Hide Options" />
                                                    <hideLarge jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/hideLarge" jcr:title="Hide on large screens" name="./hideLarge" sling:resourceType="granite/ui/components/foundation/include" />
                                                    <hideMedium jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/hideMedium" jcr:title="Hide on medium screens" name="./hideMedium" sling:resourceType="granite/ui/components/foundation/include" />
                                                    <hideSmall jcr:primaryType="nt:unstructured" path="base-app/components/content/design/cq:dialog/content/items/hideSmall" jcr:title="Hide on small screens" name="./hideSmall" sling:resourceType="granite/ui/components/foundation/include" />
                                                    <showLegalDisclaimer jcr:primaryType="nt:unstructured" text="Show Legal Disclaimer" name="./showLegalDisclaimer" sling:resourceType="granite/ui/components/coral/foundation/form/checkbox" value="true" />
                                                    <showSocial jcr:primaryType="nt:unstructured" text="Show Social" name="./showSocial" sling:resourceType="granite/ui/components/coral/foundation/form/checkbox" value="true" />
                                                    <showFooterSubMenu jcr:primaryType="nt:unstructured" text="Show Footer Sub Menu" name="./showFooterSubMenu" sling:resourceType="granite/ui/components/coral/foundation/form/checkbox" value="true" />
                                                </items>
                                            </column>
                                        </items>
                                    </config>
                                    <Design jcr:primaryType="nt:unstructured" jcr:title="Design" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" margin="{Boolean}true">

                                        <items jcr:primaryType="nt:unstructured">
                                            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <!-- Component Design changes here. -->
                                                    <bgColor jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Banner Background Color" name="./bgColor"/>
                                                    <textColor jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Text Color" name="./textColor"/>
                                                </items>
                                            </column>
                                        </items>
                                    </Design>
                                    <Social jcr:primaryType="nt:unstructured" jcr:title="Social" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">                                                  
                                                    <social-icons jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/multifield" composite="{Boolean}true" fieldLabel="Social Icons">
                                                        <field jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container" name="./socialIcons">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <social-icon-image jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathfield" fieldLabel="Social Icon Image" name="./linkLabel"/>
                                                                        <social-icon-path jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Social Icon URL" name="./linkPath"/>                                                                        
                                                                    </items>
                                                                </column>
                                                            </items>
                                                        </field>
                                                    </social-icons>
                                                </items>
                                            </column>
                                        </items>
                                    </Social>
                                    <subFooter jcr:primaryType="nt:unstructured" jcr:title="Sub-Footer" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <subMenuBgColor jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Sub-Footer Background Color" name="./subFooterBgColor"/>
                                                    <image jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathfield" fieldLabel="Logo Path" name="./fileReference" rootPath="/content/dam" />
                                                    <logoLinkPath jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathfield" fieldLabel="Logo Link Path" name="./logoLinkUrl"/>
                                                    <copyrightText jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Copyright Text" name="./copyrightText"/>
                                                    <rightsText jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Rights Text" name="./rightsText"/>
                                                    <personalInfoText jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="Personal Info Text" name="./personalInfoText"/>
                                                    <link-list jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/multifield" composite="{Boolean}true" fieldLabel="list Links">
                                                        <field jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container" name="./linkList">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <link-label jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/textfield" fieldLabel="List Link Label" name="./linkLabel"/>
                                                                        <link-path jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/form/pathfield" fieldLabel="List Link URL" name="./linkPath"/>
                                                                        <link-target jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/select" fieldLabel="Link Target" name="./linkTarget">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <option1 jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/option" selected="{Boolean}true" text="Same Tab" value="_self"/>
                                                                                <option2 jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/form/option" text="New Tab" value="_blank"/>
                                                                            </items>
                                                                        </link-target>
                                                                    </items>
                                                                </column>
                                                            </items>
                                                        </field>
                                                    </link-list>
                                                </items>
                                            </column>
                                        </items>
                                    </subFooter>
                                </items>
                            </tabs>
                        </items>
                    </column>
                </items>
            </columnProperties>
        </items>
    </content>
</jcr:root>