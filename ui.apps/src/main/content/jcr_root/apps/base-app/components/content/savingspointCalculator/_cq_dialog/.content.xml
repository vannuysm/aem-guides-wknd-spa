<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Savings Point Calculator"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="base-app.checkboxhideshow">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
            margin="{Boolean}false"/>
        <items jcr:primaryType="nt:unstructured">
            <columnProperties
                jcr:primaryType="nt:unstructured"
                jcr:title="Content"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <tabs
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                                maximized="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <content
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Content"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <caption
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Caption"
                                                        name="./contentCaption"/>
                                                    <title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Title"
                                                        name="./contentTitle"/>
                                                    <subtitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Subtitle"
                                                        name="./contentSubtitle"/>
                                                    <body
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Body"
                                                        name="./contentBody"/>
                                                    <placeholder
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Placeholder Text"
                                                        name="./contentPlaceholder"/>
                                                    <helper
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Helper Text"
                                                        name="./contentHelper"/>
                                                    <error
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Error Text"
                                                        name="./contentError"/>
                                                </items>
                                            </column>
                                        </items>
                                    </content>
                                    <calculator
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Calculator"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <calculatorCaption
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Caption"
                                                        name="./calculatorCaption"/>
                                                    <calculatorTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Title"
                                                        name="./calculatorTitle"/>
                                                    <calculatorSubtitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Subtitle"
                                                        name="./calculatorSubtitle"/>
                                                    <calculatorBody
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                        fieldLabel="Body"
                                                        name="./calculatorBody"/>
                                                    <calculatorType
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldLabel="Type"
                                                        name="./calculatorType">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <percent
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Percent"
                                                                value="percent"/>
                                                            <flat
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Flat"
                                                                value="flat"/>
                                                        </items>
                                                    </calculatorType>
                                                    <CostPer
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Cost Per"
                                                        name="./costPer"/>
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <multi
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                composite="{Boolean}true"
                                                                fieldDescription="Do not use % in savings field"
                                                                fieldLabel="Add Hidden Fields">
                                                                <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                    name="./priceRange">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <minRange
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                            fieldLabel="Min Range"
                                                                            name="./minRange"/>
                                                                        <maxRange
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                            fieldLabel="Max Range"
                                                                            name="./maxRange"/>
                                                                        <savings
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                            fieldLabel="Savings"
                                                                            name="./savings"/>
                                                                    </items>
                                                                </field>
                                                            </multi>
                                                        </items>
                                                    </well>
                                                </items>
                                            </column>
                                        </items>
                                    </calculator>
                                    <Design
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Design"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <backgroundColor
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        fieldLabel="Background Color"
                                                        name="./bgColor"
                                                        path="base-app/components/content/design/cq:dialog/content/items/bgColor"/>
                                                </items>
                                            </column>
                                        </items>
                                    </Design>
                                    <config
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Configuration"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <id
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="ID"
                                                        name="./id"/>
                                                    <wellDynamic
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <dynamicheading
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                                level="3"
                                                                text="Dynamic Content"/>
                                                            <hasDynamicContent
                                                                granite:class="showHideCheckbox"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                fieldDescription="Has Dynamic Content"
                                                                name="./hasDynamicContent"
                                                                text="Has Dynamic Content"
                                                                value="true">
                                                                <granite:data
                                                                    jcr:primaryType="nt:unstructured"
                                                                    show-hide-target=".hideTarget"/>
                                                            </hasDynamicContent>
                                                            <primarySource
                                                                granite:class="hideTarget"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                fieldLabel="Primary Source"
                                                                name="./primarySource">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <blank
                                                                        jcr:primaryType="nt:unstructured"
                                                                        text=""
                                                                        value=""/>
                                                                    <memberNumber
                                                                        jcr:primaryType="nt:unstructured"
                                                                        text="URL"
                                                                        value="url"/>
                                                                    <userName
                                                                        jcr:primaryType="nt:unstructured"
                                                                        text="Login"
                                                                        value="login"/>
                                                                </items>
                                                            </primarySource>
                                                        </items>
                                                    </wellDynamic>
                                                    <marginBottomDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Large Margin Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginBottomDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginBottomDesktop"/>
                                                    <marginTopDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Large Margin Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginTopDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginTopDesktop"/>
                                                    <paddingBottomDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Large padding Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingBottomDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingBottomDesktop"/>
                                                    <paddingTopDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Large padding Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingTopDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingTopDesktop"/>
                                                    <marginBottomtablet
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Medium Margin Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginBottomtablet"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginBottomtablet"/>
                                                    <marginToptablet
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Medium Margin Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginToptablet"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginToptablet"/>
                                                    <paddingBottomtablet
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Medium padding Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingBottomtablet"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingBottomtablet"/>
                                                    <paddingToptablet
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Medium padding Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingToptablet"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingToptablet"/>
                                                    <marginBottommobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Small Margin Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginBottommobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginBottommobile"/>
                                                    <marginTopmobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Small Margin Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginTopmobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginTopmobile"/>
                                                    <paddingBottommobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Small padding Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingBottommobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingBottommobile"/>
                                                    <paddingTopmobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Small padding Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingTopmobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingTopmobile"/>
                                                    <hideheading
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/heading"
                                                        level="3"
                                                        text="Hide Options"/>
                                                    <hideLarge
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Hide on large screens"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./hideLarge"
                                                        path="base-app/components/content/design/cq:dialog/content/items/hideLarge"/>
                                                    <hideMedium
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Hide on medium screens"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./hideMedium"
                                                        path="base-app/components/content/design/cq:dialog/content/items/hideMedium"/>
                                                    <hideSmall
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Hide on small screens"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./hideSmall"
                                                        path="base-app/components/content/design/cq:dialog/content/items/hideSmall"/>
                                                    <hideLogin
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                        name="./LoggedIn"
                                                        text="Hide When Loggedin"
                                                        value="hideLoggedIn"/>
                                                    <hideLogOut
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                        name="./LoggedOut"
                                                        text="Hide When Loggedout"
                                                        value="hideLoggedOut"/>
                                                </items>
                                            </column>
                                        </items>
                                    </config>
                                    <dynamicContent
                                        granite:class="hideTarget"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Dynamic Content"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <multifield
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <multi
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                composite="{Boolean}true"
                                                                fieldDescription="Click add to add a more Dynamic Content fields"
                                                                fieldLabel="Dynamic Content">
                                                                <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                    name="./dynamicContent">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <key
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                            fieldLabel="Key"
                                                                            name="./key"/>
                                                                        <value
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                            fieldLabel="Value"
                                                                            name="./value"/>
                                                                    </items>
                                                                </field>
                                                            </multi>
                                                        </items>
                                                    </multifield>
                                                </items>
                                            </column>
                                        </items>
                                    </dynamicContent>
                                </items>
                            </tabs>
                        </items>
                    </column>
                </items>
            </columnProperties>
        </items>
    </content>
</jcr:root>
