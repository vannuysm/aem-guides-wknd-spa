<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Dynamic-CTA Slice"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
            margin="{Boolean}false"/>
        <items jcr:primaryType="nt:unstructured">
            <columnProperties
                jcr:primaryType="nt:unstructured"
                jcr:title="Content"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <tabs
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <content
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Content"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <contentSource
                                                        granite:class="cq-dialog-dropdown-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldLabel="Content Source"
                                                        name="./contentSource">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <inline
                                                                granite:hide="${cqDesign.disableInline}"
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Inline"
                                                                value="inline"/>
                                                            <contentFragment
                                                                granite:hide="${cqDesign.disableContentFragment}"
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Content Fragment"
                                                                value="contentFragment"/>
                                                        </items>
                                                    </contentSource>
                                                    <setContentFragment
                                                        granite:class="hide list-option-listfrom-showhide-target "
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            showhidetargetvalue="contentFragment"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <well
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <multi
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                        composite="{Boolean}true"
                                                                        fieldDescription="Click '+' to add a more content fragment sources"
                                                                        fieldLabel="Configure content fragment sources">
                                                                        <field
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                            name="./cfmItems">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <link-source
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                                    fieldLabel="Link/CTA Source"
                                                                                    name="./linkSource"
                                                                                    rootPath="/content/dam/wyndham"/>
                                                                            </items>
                                                                        </field>
                                                                    </multi>
                                                                </items>
                                                            </well>
                                                        </items>
                                                    </setContentFragment>
                                                    <setInline
                                                        granite:class="hide list-option-listfrom-showhide-target "
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            showhidetargetvalue="inline"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <well
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                                     <items jcr:primaryType="nt:unstructured">
                                                                                <linkText
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                    fieldLabel="Link Label"
                                                                                    name="./linkText"/>
                                                                                
                                                                                <linkUrl
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                                    fieldLabel="Link To"
                                                                                    name="./linkUrl"
                                                                                    rootPath="/content"/>
                                                                                <openInNewWindow
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                                                    fieldDescription="Check to open link in new tab"
                                                                                    name="./linkOpenInNewWindow"
                                                                                    text="Open in New Tab?"
                                                                                    value="true"/>
                                                                        </items>
                                                            </well>
                                                        </items>
                                                    </setInline>
                                                </items>
                                            </column>
                                        </items>
                                    </content>
                                    <config
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Configuration"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <ID
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="ID"
                                                        name="./id"/>
                                                    <vertheading
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/heading"
                                                        level="3"
                                                        text="Vertical Spacing"/>
                                                    <marginBottomDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Desktop Margin Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginBottomDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginBottomDesktop"/>
                                                    <marginTopDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Desktop Margin Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginTopDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginTopDesktop"/>
                                                    <paddingBottomDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Desktop padding Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingBottomDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingBottomDesktop"/>
                                                    <paddingTopDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Desktop padding Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingTopDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingTopDesktop"/>
                                                    <marginBottomtablet
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="tablet Margin Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginBottomtablet"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginBottomtablet"/>
                                                    <marginToptablet
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="tablet Margin Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginToptablet"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginToptablet"/>
                                                    <paddingBottomtablet
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="tablet padding Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingBottomtablet"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingBottomtablet"/>
                                                    <paddingToptablet
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="tablet padding Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingToptablet"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingToptablet"/>
                                                    <marginBottommobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="mobile Margin Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginBottommobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginBottommobile"/>
                                                    <marginTopmobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="mobile Margin Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginTopmobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginTopmobile"/>
                                                    <paddingBottommobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="mobile padding Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingBottommobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingBottommobile"/>
                                                    <paddingTopmobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="mobile padding Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingTopmobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingTopmobile"/>
                                                    <hideheading
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/heading"
                                                        level="3"
                                                        text="Hide Options"/>
                                                    <hideLarge
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Hide on large screens"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./hideLarge"
                                                        path="base-app/components/content/design/cq:dialog/content/items/hideLarge"/>
                                                    <hideMedium
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Hide on medium screens"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./hideMedium"
                                                        path="base-app/components/content/design/cq:dialog/content/items/hideMedium"/>
                                                    <hideSmall
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Hide on small screens"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./hideSmall"
                                                        path="base-app/components/content/design/cq:dialog/content/items/hideSmall"/>
                                                    <hideLogin
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                        name="./LoggedIn"
                                                        text="Hide When Loggedin"
                                                        value="hideLoggedIn"/>
                                                    <hideLogOut
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                        name="./LoggedOut"
                                                        text="Hide When Loggedout"
                                                        value="hideLoggedOut"/>
                                                    <flexheading
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/heading"
                                                        level="3"
                                                        text="Horizontal align (flex)"/>
                                                    <ctaType
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldLabel="Horizontal Position"
                                                        name="./flexPosition">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <left
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Left"
                                                                value="text-left"/>
                                                            <center
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Center"
                                                                value="text-center"/>
                                                            <right
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Right"
                                                                value="text-right"/>
                                                        </items>
                                                    </ctaType>
                                                </items>
                                            </column>
                                        </items>
                                    </config>
                                    <Design
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Design"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <ctaType
                                                        granite:class="cq-dialog-dropdown-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldDescription="Describes the basic format of the link. Use of Augmented will replace ownerId and membershipId with owner-specific values in the link."
                                                        fieldLabel="CTA Size"
                                                        name="./ctaType">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <tiny
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Tiny"
                                                                value="tiny"/>
                                                            <small
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Small"
                                                                value="small"/>
                                                            <basic
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Basic"
                                                                value=""/>
                                                            <larg
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Large"
                                                                value="large"/>
                                                            <clear
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Clear"
                                                                value="clear"/>
                                                            <text
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Text"
                                                                value="text"/>
                                                            <expanded
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Expanded"
                                                                value="expanded"/>
                                                            <augmented
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Augmented"
                                                                value="augmented"/>
                                                        </items>
                                                    </ctaType>
                                                    <ctaColor
                                                        granite:class="cq-dialog-dropdown-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldLabel="CTA Color"
                                                        name="./ctaColor">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-dropdown-showhide-target=".custColorTarget"/>
                                                        <datasource
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="acs-commons/components/utilities/genericlist/datasource"
                                                            path="/etc/acs-commons/lists/dynamic-color-list"/>
                                                    </ctaColor>
                                                    <custColor
                                                        granite:class="custColorTarget"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            showhidetargetvalue="custom"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <backgroundColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Custom Button Color"
                                                                name="./customBgColor"/>
                                                        </items>
                                                    </custColor>
                                                    <fontColor
                                                        granite:class="cq-dialog-dropdown-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldLabel="CTA Font Color"
                                                        name="./fontColor">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-dropdown-showhide-target=".custfontTarget"/>
                                                        <datasource
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="acs-commons/components/utilities/genericlist/datasource"
                                                            path="/etc/acs-commons/lists/dynamic-color-list"/>
                                                    </fontColor>
                                                    <custfont
                                                        granite:class="custfontTarget"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            showhidetargetvalue="custom"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <bgColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Custom font Color"
                                                                name="./customFontColor"/>
                                                        </items>
                                                    </custfont>
                                                    <hollow
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                        fieldDescription="Hollow CTA"
                                                        name="./hollow"
                                                        text="Make the CTA hollow"
                                                        value="hollow"/>
                                                </items>
                                            </column>
                                        </items>
                                    </Design>
                                    <Analytics
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Analytics"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <EventName
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Event Name"
                                                        name="./eventName"/>
                                                    <EventCategory
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Event Category"
                                                        name="./eventCategory"/>
                                                    <EventAction
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Event Action"
                                                        name="./eventAction"/>
                                                    <EventLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Event Label"
                                                        name="./eventLabel"/>
                                                </items>
                                            </column>
                                        </items>
                                    </Analytics>
                                </items>
                            </tabs>
                        </items>
                    </column>
                </items>
            </columnProperties>
        </items>
    </content>
</jcr:root>
