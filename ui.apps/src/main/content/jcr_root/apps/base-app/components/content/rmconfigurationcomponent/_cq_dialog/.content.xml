<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="RM Configuration Component"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="base-app.checkboxhideshow">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
            margin="{Boolean}false"/>
        <items jcr:primaryType="nt:unstructured">
            <columnProperties
                jcr:primaryType="nt:unstructured"
                jcr:title="Content"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <tabs
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                                maximized="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <content
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Content"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <caption
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Caption"
                                                        name="./caption"/>
                                                    <title1
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Additional Info Title 1"
                                                        name="./title1"/>
                                                    <body1
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Additional Info Body 1"
                                                        name="./body1"/>
                                                    <title2
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Additional Info Title 2"
                                                        name="./title2"/>
                                                    <body2
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Additional Info Body 2"
                                                        name="./body2"/>
                                                    <titlecategory
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <multi
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                composite="{Boolean}true"
                                                                fieldDescription="Click add to add a more Category Title "
                                                                fieldLabel="Category Title Section">
                                                                <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                    name="./categories">
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <key
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                            fieldLabel="Category Title "
                                                                            name="./title"/>
                                                                    </items>
                                                                </field>
                                                            </multi>
                                                        </items>
                                                    </titlecategory>
                                                </items>
                                            </column>
                                        </items>
                                    </content>
                                    <Design
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Design"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <backgroundColor
                                                        granite:class="cq-dialog-dropdown-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldLabel="Additional Info Background Color"
                                                        name="./additionalInfoBackgroundColor">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-dropdown-showhide-target=".custConfigColorTarget"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <Default
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Default Color"
                                                                value=""/>
                                                            <primary
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Primary"
                                                                value="primary"/>
                                                            <secondary
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Secondary"
                                                                value="secondary"/>
                                                            <success
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Success"
                                                                value="success"/>
                                                            <alert
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Alert"
                                                                value="alert"/>
                                                            <warning
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Warning"
                                                                value="warning"/>
                                                            <color1
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color1"
                                                                value="color1"/>
                                                            <color2
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color2"
                                                                value="color2"/>
                                                            <color3
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color3"
                                                                value="color3"/>
                                                            <color4
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color4"
                                                                value="color4"/>
                                                            <color5
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color5"
                                                                value="color5"/>
                                                            <color6
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color6"
                                                                value="color6"/>
                                                            <color7
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color7"
                                                                value="color7"/>
                                                            <color8
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color8"
                                                                value="color8"/>
                                                            <color9
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color9"
                                                                value="color9"/>
                                                            <color10
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color10"
                                                                value="color10"/>
                                                            <color11
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color11"
                                                                value="color11"/>
                                                            <color12
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color12"
                                                                value="color12"/>
                                                            <color13
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color13"
                                                                value="color13"/>
                                                            <color14
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color14"
                                                                value="color14"/>
                                                            <color15
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color15"
                                                                value="color15"/>
                                                            <color16
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color16"
                                                                value="color16"/>
                                                            <custom
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Custom"
                                                                value="custom"/>
                                                        </items>
                                                    </backgroundColor>
                                                    <customBackgroundColor
                                                        granite:class="custConfigColorTarget"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            showhidetargetvalue="custom"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <customColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Custom Background Color"
                                                                name="./customAdditionalInfoBackgroundColor"/>
                                                        </items>
                                                    </customBackgroundColor>
                                                    <categoryTitleColor
                                                        granite:class="cq-dialog-dropdown-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldLabel="Category Title Color"
                                                        name="./categoryTitleColor">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-dropdown-showhide-target=".custCategoryTitleColorTarget"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <Default
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Default Color"
                                                                value=""/>
                                                            <primary
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Primary"
                                                                value="primary"/>
                                                            <secondary
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Secondary"
                                                                value="secondary"/>
                                                            <success
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Success"
                                                                value="success"/>
                                                            <alert
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Alert"
                                                                value="alert"/>
                                                            <warning
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Warning"
                                                                value="warning"/>
                                                            <color1
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color1"
                                                                value="color1"/>
                                                            <color2
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color2"
                                                                value="color2"/>
                                                            <color3
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color3"
                                                                value="color3"/>
                                                            <color4
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color4"
                                                                value="color4"/>
                                                            <color5
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color5"
                                                                value="color5"/>
                                                            <color6
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color6"
                                                                value="color6"/>
                                                            <color7
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color7"
                                                                value="color7"/>
                                                            <color8
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color8"
                                                                value="color8"/>
                                                            <color9
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color9"
