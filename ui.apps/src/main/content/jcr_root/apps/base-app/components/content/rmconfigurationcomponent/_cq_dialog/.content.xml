                                                            <color10
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color10"
                                                                value="color10"/>
                                                            <color11
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color11"
                                                                value="color11"/>
                                                            <color12
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color12"
                                                                value="color12"/>
                                                            <color13
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color13"
                                                                value="color13"/>
                                                            <color14
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color14"
                                                                value="color14"/>
                                                            <color15
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color15"
                                                                value="color15"/>
                                                            <color16
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color16"
                                                                value="color16"/>
                                                            <custom
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Custom"
                                                                value="custom"/>
                                                        </items>
                                                    </categoryTitleColor>
                                                    <categoryTitleCustomBackgroundColor
                                                        granite:class="custCategoryTitleColorTarget"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            showhidetargetvalue="custom"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <customColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Custom Background Color"
                                                                name="./customCategoryTitleColor"/>
                                                        </items>
                                                    </categoryTitleCustomBackgroundColor>
                                                </items>
                                            </column>
                                        </items>
                                    </Design>
                                    <config
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Configuration"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <id
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="ID"
                                                        name="./id"/>
                                                    <marginBottomDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Large Margin Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginBottomDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginBottomDesktop"/>
                                                    <marginTopDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Large Margin Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginTopDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginTopDesktop"/>
                                                    <paddingBottomDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Large padding Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingBottomDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingBottomDesktop"/>
                                                    <paddingTopDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Large padding Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingTopDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingTopDesktop"/>
                                                    <marginBottomtablet
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Medium Margin Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginBottomtablet"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginBottomtablet"/>
                                                    <marginToptablet
