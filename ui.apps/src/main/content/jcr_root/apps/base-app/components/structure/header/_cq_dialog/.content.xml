                                    toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,#styles,#paraformat,image#imageProps,#justify,#lists,links#modifylink,links#unlink,links#anchor,misctools#specialchars,misctools#sourceedit,#fullscreen,edit#cut,edit#copy,edit#paste-default,edit#paste-plaintext,table#table]">
