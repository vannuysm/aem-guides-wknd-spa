                                    <rootPath
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Select path where to start navigation menu"
                                        fieldLabel="Navigation Root Path"
                                        name="./navigationRootPath"
                                        rootPath="/content/wyndham"/>
                                    <depth
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldDescription="(eg: 1 for 1 level navigation, 2 for 2 level navigation)"
                                        fieldLabel="Depth of the navigation level"
                                        max="3"
                                        min="1"
                                        name="./navigationDepth"
                                        step="1"
                                        value="3"/>
                                </items>
                            </column>
                        </items>
                    </navigation>
