                                                                                                <wyn-type-title-1 jcr:primaryType="nt:unstructured" cssName="wyn-type-title-1" text="Title 1" />
                                                                                                <wyn-type-title-2 jcr:primaryType="nt:unstructured" cssName="wyn-type-title-2" text="Title 2" />
