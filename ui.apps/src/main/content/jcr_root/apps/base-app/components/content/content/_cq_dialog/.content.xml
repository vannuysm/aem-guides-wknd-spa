                                                                                                <wyn-image--left jcr:primaryType="nt:unstructured" cssName="wyn-image--left" text="Image Left" />
                                                                                                <wyn-image--right jcr:primaryType="nt:unstructured" cssName="wyn-image--right" text="Image Right" />
