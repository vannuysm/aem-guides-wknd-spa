                                                                                                <wyn-type-subheading-1 jcr:primaryType="nt:unstructured" cssName="wyn-type-subheading-1" text="SubHeading 1" />
                                                                                                <wyn-type-subheading-2 jcr:primaryType="nt:unstructured" cssName="wyn-type-subheading-2" text="SubHeading 2" />
                                                                                                <wyn-type-caption jcr:primaryType="nt:unstructured" cssName="wyn-type-caption" text="Caption" />
                                                                                                <wyn-type-body-1 jcr:primaryType="nt:unstructured" cssName="wyn-type-body-1" text="Regular Text" />
