<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="AssesmentSummaryDetail"
    sling:resourceType="cq/gui/components/authoring/dialog"
    mode="edit">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
            margin="{Boolean}false"/>
        <items jcr:primaryType="nt:unstructured">
            <columnProperties
                jcr:primaryType="nt:unstructured"
                jcr:title="Content"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <tabs
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <content
                                        granite:class="generic-dialog--editor"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Content"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Caption"
                                                        name="./title"/>
                                                    <titlePdf
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Subtitle"
                                                        name="./titlePdf"/>
                                                </items>
                                            </column>
                                        </items>
                                    </content>
                                    <cta
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="CTA"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <ctaLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="CTA Label"
                                                        name="./ctaLabel"/>
                                                    <ctaType
                                                        granite:class="cq-dialog-dropdown-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldDescription="Describes the basic format of the link. Use of Augmented will replace ownerId and membershipId with owner-specific values in the link."
                                                        fieldLabel="CTA Size"
                                                        name="./ctaType">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <tiny
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Tiny"
                                                                value="tiny"/>
                                                            <small
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Small"
                                                                value="small"/>
                                                            <basic
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Basic"
                                                                value=""/>
                                                            <larg
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Large"
                                                                value="large"/>
                                                            <clear
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Clear"
                                                                value="clear"/>
                                                            <text
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Text"
                                                                value="text"/>
                                                            <expanded
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Expanded"
                                                                value="expanded"/>
                                                            <augmented
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Augmented"
                                                                value="augmented"/>
                                                        </items>
                                                    </ctaType>
                                                    <ctaColor
                                                        granite:class="cq-dialog-dropdown-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldLabel="CTA Color"
                                                        name="./ctaColor">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-dropdown-showhide-target=".custColorTarget"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <Default
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Default Color"
                                                                value=""/>
                                                            <primary
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Primary"
                                                                value="primary"/>
                                                            <secondary
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Secondary"
                                                                value="secondary"/>
                                                            <success
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Success"
                                                                value="success"/>
                                                            <alert
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Alert"
                                                                value="alert"/>
                                                            <warning
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Warning"
                                                                value="warning"/>
                                                            <color1
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color1"
                                                                value="color1"/>
                                                            <color2
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color2"
                                                                value="color2"/>
                                                            <color3
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color3"
                                                                value="color3"/>
                                                            <color4
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color4"
                                                                value="color4"/>
                                                            <color5
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color5"
                                                                value="color5"/>
                                                            <color6
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color6"
                                                                value="color6"/>
                                                            <color7
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color7"
                                                                value="color7"/>
                                                            <color8
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color8"
                                                                value="color8"/>
                                                            <color9
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color9"
                                                                value="color9"/>
                                                            <color10
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color10"
                                                                value="color10"/>
                                                            <color11
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color11"
                                                                value="color11"/>
                                                            <color12
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color12"
                                                                value="color12"/>
                                                            <color13
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color13"
                                                                value="color13"/>
                                                            <color14
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color14"
                                                                value="color14"/>
                                                            <color15
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color15"
                                                                value="color15"/>
                                                            <color16
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color16"
                                                                value="color16"/>
                                                            <custom
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Custom"
                                                                value="custom"/>
                                                        </items>
                                                    </ctaColor>
                                                    <custColor
                                                        granite:class="custColorTarget "
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            showhidetargetvalue="custom"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <fontColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Button Font Color"
                                                                name="./fontColor"/>
                                                            <backgroundColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/foundation/include"
                                                                fieldLabel="Custom Button Color"
                                                                name="./bgColor"
                                                                path="base-app/components/content/design/cq:dialog/content/items/bgColor"/>
                                                        </items>
                                                    </custColor>
                                                    <hollow
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                        fieldDescription="Hollow CTA"
                                                        name="./hollow"
                                                        text="Make the CTA hollow"
                                                        value="hollow"/>
                                                </items>
                                            </column>
                                        </items>
                                    </cta>
                                    <design
                                        granite:class="generic-dialog--editor"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Design"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <backgroundColor
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Background Color"
                                                        name="./backColor"/>
                                                </items>
                                            </column>
                                        </items>
                                    </design>
                                    <config
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Configuration"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <marginBottomDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Desktop Margin Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginBottomDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginBottomDesktop"/>
                                                    <marginTopDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Desktop Margin Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginTopDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginTopDesktop"/>
                                                    <paddingBottomDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Desktop padding Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingBottomDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingBottomDesktop"/>
                                                    <paddingTopDesktop
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Desktop padding Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingTopDesk"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingTopDesktop"/>
                                                    <marginBottomtablet
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="tablet Margin Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginBottomtablet"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginBottomtablet"/>
                                                    <marginToptablet
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="tablet Margin Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginToptablet"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginToptablet"/>
                                                    <paddingBottomtablet
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="tablet padding Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingBottomtablet"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingBottomtablet"/>
                                                    <paddingToptablet
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="tablet padding Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingToptablet"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingToptablet"/>
                                                    <marginBottommobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="mobile Margin Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginBottommobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginBottommobile"/>
                                                    <marginTopmobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="mobile Margin Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginTopmobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginTopmobile"/>
                                                    <paddingBottommobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="mobile padding Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingBottommobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingBottommobile"/>
                                                    <paddingTopmobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="mobile padding Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingTopmobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingTopmobile"/>
                                                    <hideheading
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/heading"
                                                        level="3"
                                                        text="Hide Options"/>
                                                    <hideLarge
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Hide on large screens"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./hideLarge"
                                                        path="base-app/components/content/design/cq:dialog/content/items/hideLarge"/>
                                                    <hideMedium
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Hide on medium screens"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./hideMedium"
                                                        path="base-app/components/content/design/cq:dialog/content/items/hideMedium"/>
                                                    <hideSmall
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Hide on small screens"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./hideSmall"
                                                        path="base-app/components/content/design/cq:dialog/content/items/hideSmall"/>
                                                    <hideLogOut
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                        name="./LoggedOut"
                                                        text="Hide When Logged out"
                                                        value="hideLoggedOut"/>
                                                    <hideLogin
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                        name="./LoggedIn"
                                                        text="Hide When Logged in"
                                                        value="hideLoggedIn"/>
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="block"/>
                                                </items>
                                            </column>
                                        </items>
                                    </config>
                                </items>
                            </tabs>
                        </items>
                    </column>
                </items>
            </columnProperties>
        </items>
    </content>
</jcr:root>
