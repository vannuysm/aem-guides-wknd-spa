                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldLabel="Calendar Title Background Color"
                                                        name="./calendarTitleBgColor">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-dropdown-showhide-target=".custColorTarget"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <Default
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Default Color"
                                                                value=""/>
                                                            <primary
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Primary"
                                                                value="primary"/>
                                                            <secondary
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Secondary"
                                                                value="secondary"/>
                                                            <success
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Success"
                                                                value="success"/>
                                                            <alert
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Alert"
                                                                value="alert"/>
                                                            <warning
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Warning"
                                                                value="warning"/>
                                                            <color1
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color1"
                                                                value="color1"/>
                                                            <color2
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color2"
                                                                value="color2"/>
                                                            <color3
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color3"
                                                                value="color3"/>
                                                            <color4
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color4"
                                                                value="color4"/>
                                                            <color5
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color5"
                                                                value="color5"/>
                                                            <color6
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color6"
                                                                value="color6"/>
                                                            <color7
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color7"
                                                                value="color7"/>
                                                            <color8
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color8"
                                                                value="color8"/>
                                                            <color9
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color9"
                                                                value="color9"/>
                                                            <color10
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color10"
                                                                value="color10"/>
                                                            <color11
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color11"
                                                                value="color11"/>
                                                            <color12
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color12"
                                                                value="color12"/>
                                                            <color13
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color13"
                                                                value="color13"/>
                                                            <color14
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color14"
                                                                value="color14"/>
                                                            <color15
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color15"
                                                                value="color15"/>
                                                            <color16
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color16"
                                                                value="color16"/>
                                                            <custom
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Custom"
                                                                value="custom"/>
                                                        </items>
                                                    </calendarTitleBgColor>
                                                    <customCalendarTitleBgColor
                                                        granite:class="custColorTarget "
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            showhidetargetvalue="custom"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <customColor
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Custom Background Color"
                                                                name="./customCalendarTitleBgColor"/>
                                                        </items>
                                                    </customCalendarTitleBgColor>
                                                    <textLinkColor
                                                        granite:class="cq-dialog-dropdown-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldLabel="Text Link Color"
                                                        name="./textLinkColor">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-dropdown-showhide-target=".custBgColorTarget"/>
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <Default
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Default Color"
                                                                value=""/>
                                                            <primary
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Primary"
                                                                value="primary"/>
                                                            <secondary
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Secondary"
                                                                value="secondary"/>
                                                            <success
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Success"
                                                                value="success"/>
                                                            <alert
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Alert"
                                                                value="alert"/>
                                                            <warning
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Warning"
                                                                value="warning"/>
                                                            <color1
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color1"
                                                                value="color1"/>
                                                            <color2
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color2"
                                                                value="color2"/>
                                                            <color3
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color3"
                                                                value="color3"/>
                                                            <color4
                                                                jcr:primaryType="nt:unstructured"
                                                                text="color4"
