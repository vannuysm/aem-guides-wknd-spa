                                                        name="./paddingToptablet"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingToptablet"/>
                                                    <marginBottommobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Small Margin Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginBottommobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginBottommobile"/>
                                                    <marginTopmobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Small Margin Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./marginTopmobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/marginTopmobile"/>
                                                    <paddingBottommobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Small padding Bottom"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingBottommobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingBottommobile"/>
                                                    <paddingTopmobile
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Small padding Top"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./paddingTopmobile"
                                                        path="base-app/components/content/design/cq:dialog/content/items/paddingTopmobile"/>
                                                    <hideheading
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/heading"
                                                        level="3"
                                                        text="Hide Options"/>
                                                    <hideLarge
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Hide on large screens"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./hideLarge"
                                                        path="base-app/components/content/design/cq:dialog/content/items/hideLarge"/>
                                                    <hideMedium
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Hide on medium screens"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./hideMedium"
                                                        path="base-app/components/content/design/cq:dialog/content/items/hideMedium"/>
                                                    <hideSmall
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="Hide on small screens"
                                                        sling:resourceType="granite/ui/components/foundation/include"
                                                        name="./hideSmall"
                                                        path="base-app/components/content/design/cq:dialog/content/items/hideSmall"/>
                                                    <hideLogin
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                        name="./LoggedIn"
                                                        text="Hide When Loggedin"
                                                        value="hideLoggedIn"/>
                                                    <hideLogOut
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
